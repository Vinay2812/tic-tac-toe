{
    "version": 2,
    "builds": [
      {
        "src": "/index.js", 
        "use": "@vercel/node"
      },
      {
        "src": "/client/build",  
        "use": "@vercel/client/build/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
}